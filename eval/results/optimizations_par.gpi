set autoscale
set logscale xy
set xtic auto
set ytic auto

set title "Comparing the optimization flags for the parallel version"
set xlabel "Size of the array"
set ylabel "Computation time (s)"

set term png
set output strftime('optimizations_par/%F_%H-%M-%S.png', time(0))

plot "parO0.dat" using 1:2:3 title 'Parallel O0' with yerrorbars linetype 1, \
    "" using 1:2 notitle with lines linetype 1, \
    "parO1.dat" using 1:2:3 title 'Parallel O1' with yerrorbars linetype 2, \
    "" using 1:2 notitle with lines linetype 2, \
    "parO2.dat" using 1:2:3 title 'Parallel O2' with yerrorbars linetype 3, \
    "" using 1:2 notitle with lines linetype 3, \
    "parO3.dat" using 1:2:3 title 'Parallel O3' with yerrorbars linetype 4, \
    "" using 1:2 notitle with lines linetype 4

